includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - app/
        - config/
        - database/
        - tests/
    excludePaths:
        - resources/views/vendor

    level: 5

    reportUnmatchedIgnoredErrors: false
    ignoreErrors:
        - '#PHPDoc type#'
        - '#Call to an undefined method Faker#'
        - '#to an undefined property App\\Http\\Resources#'
        - '#to an undefined method App\\Http\\Resources#'
